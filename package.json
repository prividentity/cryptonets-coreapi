{
  "name": "cryptonets-coreapi",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "tsc && ts-node src/app.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check",
    "dev": "nodemon",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "yarn prettier:fix",
      "yarn lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openinfer/cryptonets-coreapi.git"
  },
  "author": "sriesow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/openinfer/cryptonets-coreapi/issues"
  },
  "homepage": "https://github.com/openinfer/cryptonets-coreapi#readme",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^16.0.0",
    "@types/pngjs": "^6.0.1",
    "@types/request-promise": "^4.1.48",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "^4.17.1",
    "express-prettify": "^0.1.2",
    "express-winston": "^4.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^1.18.9",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "ffi-napi": "^4.0.3",
    "pngjs": "^6.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "4.6.1"
